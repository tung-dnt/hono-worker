    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20241011.0",
    "@hono/node-server": "^1.13.2",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.8",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.25.0",
